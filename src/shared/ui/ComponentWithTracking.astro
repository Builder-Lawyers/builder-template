---
import type { HTMLAttributes } from "astro/types";

type RenderFn<Props> = (
  props: Props,
  inject: (key: string) => HTMLAttributes<any>,
) => any;

interface PropsWithTracking<Props> {
  props: Props;
  render: RenderFn<Props>;
}

const { props, render } = Astro.props as PropsWithTracking<any>;

const inject = (key: string): HTMLAttributes<any> => ({ "data-props": key });
---

{render(props, inject)}
